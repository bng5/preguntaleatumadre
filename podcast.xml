---
---
<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0"
  xmlns:atom="http://www.w3.org/2005/Atom"
  xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd">
 <channel>
  <atom:link href="{{ site.url }}/podcast.xml" rel="self" type="application/rss+xml" />
  <title>{{ site.title | xml_escape }}</title>
  <link>{{ site.url | xml_escape }}</link>
  <description>{{ site.tagline | xml_escape }}</description>
  <lastBuildDate>{{ 'now' | date_to_rfc822 }}</lastBuildDate>
  <language>es-UY</language>
  <image>
   <title>{{ site.title }}</title>
   <url>{{ site.url }}/assets/images/patum.png</url>
   <link>{{ site.url }}</link>
  </image>
  <itunes:type>episodic</itunes:type>
  <itunes:category text="Comedy" />
  <itunes:explicit>clean</itunes:explicit>
  <itunes:author>Mediarte</itunes:author>
  {%- for post in site.posts %}
  <item>
   <title>{{ post.title }}</title>
   <link>{{ site.url }}/episodios/{{ post.file }}</link>
   <description>{{ post.content | rstrip | xml_escape }}</description>
   <pubDate>{{ post.pubDate | date_to_rfc822 }}</pubDate>
   <category>Podcast</category>
   <guid isPermaLink="true">{{ site.url }}/episodios/{{ post.file }}</guid>
   <enclosure url="{{ site.url }}/episodios/{{ post.file }}" length="{{ post.length }}" type="audio/mpeg" />
   <itunes:episode>{{ post.episode }}</itunes:episode>
   <itunes:episodeType>full</itunes:episodeType>
   <itunes:duration>{{ post.duration }}</itunes:duration>
   <itunes:explicit>clean</itunes:explicit>
  </item>
  {%- endfor %}
 </channel>
</rss>
